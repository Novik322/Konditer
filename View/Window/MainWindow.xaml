<Window x:Class="Konditer_FigmaProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Konditer_FigmaProject"
        mc:Ignorable="d"
        Title="Кондитерская" Height="800" Width="420" WindowStartupLocation="CenterScreen" AllowsTransparency="True" Background="Transparent"
        WindowStyle="None" ResizeMode="NoResize">

    <Grid>
        <Image  Grid.Row="1" Grid.Column="1" Source="/View/Picture/testphoto.png" Stretch="Fill" />
        <Frame Grid.Column="1" Grid.Row="1" Name="FrameGlav" NavigationUIVisibility="Hidden"/>

        <Border CornerRadius="10">
            <StackPanel Orientation="Vertical" VerticalAlignment="Bottom">
                <Rectangle Fill="#293133" Width="200" Height="10" Margin="22" MouseLeftButtonDown="Rectangle_MouseLeftButtonDown"/>
            </StackPanel>
        </Border>
    </Grid>
</Window>